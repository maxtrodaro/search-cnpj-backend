		"express": "^4.17.3",
		"factory-girl": "^5.0.4",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.20.13",
		"pg": "^7.18.2",
		"pg-hstore": "^2.3.3",
		"sequelize": "^5.21.7",
		"yup": "^0.28.4"
	},
	"devDependencies": {
		"faker": "^4.1.0",
		"jest": "^25.2.4",
